function _clearmap()
 srand(time())
 for y=0,ylimit-1 do
  for x=0,xlimit-1 do
   mset(x,y,0)
  end
 end
 for room=0,maxrooms do
  del(mpx,mpx[room])
  del(mpy,mpx[room])
  del(mph,mpx[room])
  del(mpw,mpx[room])
 end
end

function _gloop()
 if gamestate == 40 then
  _ply()
  if playermoved == 1 then 
   _npc() 
   lastmess -= 1
  end
  if health<1 then 
   _mess("you died")
   gamestate = 42 
   wait = 0
  end
 end
 if gamestate == 41 then
	 _clearmap()
  level+=1
  srand(time())
  gamestate=30
	end
 if gamestate == 42 then
  if wait<250 then
   if wait%15 then _npc() end
   wait += 1
  else
 	 _clearmap()
   level+=1  
   gamestate=10
  end
	end	
	
end

function _tick_over()
