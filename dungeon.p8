pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	gamemode = 0

end

-- update the position based on the player pressing buttons
function _update()
	if gamemode == 0 then _intro() end	
	if gamemode == 1 then _titlescreen() end	
	if gamemode == 2 then _drawlevel() end	
	if gamemode == 3 then _maingameloop() end	
	if gamemode == 4 then _nextlevel() end	
end



function _intro() 	
 gamemode = 1
end

function _titlescreen() 
 gamemode = 2
end	

function _drawlevel() 
 for y=0, 63 do
  for x=0, 63 do
    if mget(x,y)==12 then 
      px = x
      py = y
      mset(x,y,1)
    end
  end
 end
 keys = 0
 potions = 0
 coins = 0
 --py = 8
 gamemode = 3
end	


function _nextlevel() 
end	

function	_maingameloop() 
 nx = px
 ny = ny
	if btnp(0) then nx = px - 1 end
	if btnp(1) then nx = px + 1 end
	if btnp(2) then ny = py - 1 end
	if btnp(3) then ny = py + 1 end
 -- if floor or opendoor move.
	if mget(nx,ny) == 1 or mget(nx,ny) == 6 then 
	 px = nx
	 py = ny
	end
 -- open closed door
	if mget(nx,ny) == 5 then
		mset(nx,ny,6)
	end
	-- open locked door
	if mget(nx,ny) == 7 and keys > 0 then
  keys -= 1
		mset(nx,ny,6)
	end
	-- if it's a pick up - pick it up and move
	if mget(nx,ny) > 16 and mget(nx,ny) <48 then
	 px = nx
	 py = ny
	 -- if it's key add a key
  if mget(nx,ny) == 18 then keys += 1 end
	 -- replace with floor tile
	 mset(nx,ny,1)
	end
end	

-- clear the screen, then draw a circle at the current position
function _draw()
	cls(0)
	ox = px-7
	oy = py-7
	if ox < 1 then ox = 1 end
	if oy < 1 then oy = 1 end
	if ox > 48 then ox = 48 end
	if oy > 48 then oy = 48 end
	for y=0, 15 do
		for x=0, 15 do
			if mget(ox+x,oy+y) > 16 then
				spr (1,x*8,y*8)
			end	
		 map(ox+x,oy+y,x*8,y*8,1,1)
			--spr (map1[oy+y][ox+x],x*8,y*8)
	  end
	end 
	spr (12,(px-ox)*8,(py-oy)*8)
	print ('health '..' keys '..keys..' level '..px..' '..py,0.0)

end

