pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	gamemode = 0
 item = {"potion","key","coin","spell book","spell book","the diamond!","knife","sword","big sword","axe","big axe","armour","nice armour","great armour"}
end

-- update the position based on the player pressing buttons
function _update()
	if gamemode == 0 then _intro() end	
	if gamemode == 1 then _titlescreen() end	
	if gamemode == 2 then _drawlevel() end	
	if gamemode == 3 then _maingameloop() end	
	if gamemode == 4 then _nextlevel() end	
end



function _intro() 	
 gamemode = 1
end

function _titlescreen() 
 gamemode = 2
end	

function _drawlevel() 
 for y=0, 63 do
  for x=0, 63 do
    if mget(x,y)==12 then 
      px = x
      py = y
      mset(x,y,1)
    end
  end
 end
 keys = 0
 spells = 0
 coins = 0
 level = 1 
 dung = 1
 atk = 1
 def = 1
 potions = 1
 maxhealth = 10
 health = 10
 diamond = 0
 gamemode = 3
end	


function _nextlevel() 
end	

function	_maingameloop()
 _ply()
end 

function	_ply() 
 nx = px
 ny = py
	if btnp(0) then nx = px - 1 end
	if btnp(1) then nx = px + 1 end
	if btnp(2) then ny = py - 1 end
	if btnp(3) then ny = py + 1 end
 -- if floor or opendoor move.
	if mget(nx,ny) == 1 or mget(nx,ny) == 6 then 
	 px = nx
	 py = ny
	end
 -- open closed door
	if mget(nx,ny) == 5 then
		mset(nx,ny,6)
		mess = "unlocked door"
	end
	-- open locked door
	if mget(nx,ny) == 7 and keys > 0 then
  keys -= 1
		mess = "use key to unlock door"
		mset(nx,ny,6)
	end
	-- if it's a pick up - pick it up and move
	if mget(nx,ny) > 16 and mget(nx,ny) <48 then
	 px = nx
	 py = ny
		mess = "picked up "..item[mget(nx,ny)-16]
	 -- if it's poiion restore health
  if mget(nx,ny) == 17 then health = maxhealth end
	 -- if it's key add a key
  if mget(nx,ny) == 18 then keys += 1 end
	 -- if it's key add a coin
  if mget(nx,ny) == 19 then coins += 1 end
	 -- if it's spell book add a spellbook
  if mget(nx,ny) == 20 then spells += 1 end
	 -- if it's spell book add a spellbook
  if mget(nx,ny) == 21 then 
   level +=1
		 maxhealth = maxhealth*1.2
			health = maxhealth
  end
	 -- if it's diamond add a diamond
  if mget(nx,ny) == 22 then diamond += 1 end
  -- if it's weapon add atk score
  if mget(nx,ny) > 22 and mget(nx,ny) < 28 then 
  	atk += 1 
  end
  -- if it's weapon add atk score
  if mget(nx,ny) > 27 and mget(nx,ny) < 31 then 
  	def += 1 
  end



	 -- replace with floor tile
	 mset(nx,ny,1)
	end
end	

-- clear the screen, then draw a circle at the current position
function _draw()
	cls(0)
	ox = px-7
	oy = py-7
	if ox < 1 then ox = 0 end
	if oy < 1 then oy = 0 end
	if ox > 48 then ox = 48 end
	if oy > 48 then oy = 48 end
	for y=0, 15 do
		for x=0, 15 do
			if mget(ox+x,oy+y) > 16 then
				spr (1,x*8,y*8)

			end	
		 map(ox+x,oy+y,x*8,y*8,1,1)
			--spr (map1[oy+y][ox+x],x*8,y*8)
	  end
	end 
	spr (12,(px-ox)*8,(py-oy)*8)
	_gui()
end

function _gui()
 spr (13,0,0)
	for a=1,14 do
		spr (14,a*8,0)
	end
	spr (15,120,0)
	-- player level
	spr (12,8,0)
	print (level,16,1)
	-- dungeon
	spr (4,24,0)
	print (dung,33,1)
	-- keys
	spr (18,38,0)
	print (keys,44,1)
 -- coins
	spr (19,50,0)
	print (coins,58,1)
	-- potions 
 spr (17,64,0)
	print (potions,71,1) 
	-- spells 
 --	spr (20,56,0)
	-- print (spells,64,1)
	--atk
	spr (24,76,0)
	print (atk,84,1)
	--def
	spr (29,90,0)
	print (def,98,1)
	--heath
	spr (11,104,0)
	print (health,112,1)
	-- print ('health '..' keys '..keys..' level '..px..' '..py,0.0)
	-- message
 spr (9,0,120)
	for a=1,14 do
		spr (14,a*8,120)
	end
	spr (10,120,120)	
	if mess then print(mess,8,121) end
end
