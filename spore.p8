-- draw

function _draw()
	cls(0)
 --- draw title screen
 if gamemode == 1 then _titlescreen() end 
 if gamemode == 3.5 then _levsta() end  
 if gamemode == 4.5 then _levend() end  
 if gamemode == 5.5 then _gamend() end  
 -- draw main gamemode
 if gamemode == 4 then _drawgame() end  
end

function _drawgame()
  map(0,0,0,0,16,15)
  _gui()
end

function _levsta()
  map(8-flr(sd),8-flr(sd),64-(sd*8),64-(sd*8),sd*2,sd*2)
  sd += 1
  if sd>8 then gamemode = 4 end
end

function _levend()
  map(8-flr(sd),8-flr(sd),64-(sd*8),64-(sd*8),sd*2,sd*2)
  sd -= 1
  if sd<1 then
   level += 1 
   gamemode = 3
  end
end

function _gamend()
  map(8-flr(sd),8-flr(sd),64-(sd*8),64-(sd*8),sd*2,sd*2)
  sd -= 1
  if sd<1 then
   gamemode = 1
  end
end


function _gui()
 print("energy "..health,0,122)
 print(potions,63,122)
 spr(28,54,120)
 print(barrels.."/"..maxbarrels.." barrels",76,122)
end


function _titlescreen()
 map(115,59,12,16,13,5)
 _text("press ❎ to start",32,100,7)
 _text("original by jim baguley",32,112,7)
 _text("pico-8 version by glen mcnamee",32,120,7)
 print(btnp(4),0,0)
  print(gamemode,32,0)
end

function _text(str,x,y,c)
 x=64-(#str*2)
 print (str,x,y+1,5)
 print (str,x,y,c)
end
