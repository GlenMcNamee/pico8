pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
  xpos = 0
  ypos = 8
  h={}
//  h= {2,2,3,4,5,6,3,5,4,6,2,2,4,2,0}

  h[0]=0
  bomb = 0
  gamemode = 0;
  for x=1,14 do
   h[x] = 2+flr(rnd(6))
   print (h.x)
  end
  
  h[15]=0
end

-- update the position based on the player pressing buttons
function _update()
			if gamemode == 0 then _intro() end	
			if gamemode == 1 then _gameloop() end	

end

function _intro()
	  gamemode = 1
	  _initlevel()
	  
end

function _initlevel()
	  
end


function _gameloop()
		_moveplayer()
  _movebomb()
		_checkcontroller()
end

function _moveplayer()
		-- move player
		xpos += 1
		if (xpos > 128) then
			xpos -= 128+16  
			ypos += 8
		end
end

function _movebomb()
		-- move bomb
		if (bomb==1) then bomby+=1
			if bomby>128 then bomb=0	
			end
		end
end

function _checkcontroller()
		-- check controller
		if (btn(4) or btn(5)) then
			if (bomb == 0) then
			 bomby = ypos + 8
			 bombx = flr(xpos/8)*8
				bomb = 1
			end
		end
end

-- clear the screen, then draw a circle at the current position
function _draw()
  cls()
  if gamemode == 1 then
    _drawplayer()
		  _drawcity()
		  _drawbomb()
		end
end

function _drawplayer()
  spr (48,xpos,ypos,2,1)
end

function _drawcity()
		--loop for each column
  for x=0,15 do
  --draw grass
			spr (1,x*8,120)
			y=h[x]
			--draw buildings
			bx = x-1
			if (bx > 7 ) then bx-=8 end
					for xx=1,y do					
						if (xx == y) then 
						 --draw roof
						 spr (18+bx,x*8,120-(xx*8))
						else
						 --draw floors
						 spr (2+bx,x*8,120-(xx*8))
						end
					end
					--print (h.x,x*8,0)
		end
end

function _drawbomb()
  --draw bomb
		if (bomb==1) then
			spr (50,bombx,bomby)
		end
end	
	

