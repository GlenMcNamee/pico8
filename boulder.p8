pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	gamemode = 0
	dr = {1,-1,0,0}
 title = {"bouldercraft","by glen mcnamee","","use ”ƒ‹‘ to move", " ", "","","","press — to start"}
 endgame = {"well done","","you've completed","dungeon penetrator","","","sorry it's sucks","i couldn't finish it in time!","","press â€” to continue"}

end

function _intro() 	
 last = time()
	gamemode = 1
end

function _titlescreen() 

 if btnp(4) and time() > last then
  gamemode = 2
 end
end	

function _initgame()
 diamonds = 00
 level = 1 
 lives = 3
 _drawlevel() 
 gamemode = 3
end

function _drawlevel() 
 npc = 0
 npcx = {} -- npc y
 npcy = {} -- npc x
 npcd = {} -- npc direction
 lx = (level) * 16 -- ofset for different dungeons
 ly = 0
 if level > 7 then 
  ly = 16
  lx = (dung-8) * 32
 end
 for y=0, 15 do
  for x=0, 15 do
   mset(x,y,mget(lx+x,ly+y))
   if mget(lx+x,ly+y)==1 then 
    px = x
    py = y
    --mset(x,y,1)
   end
   if mget(lx+x,ly+y)>16 then
    npcx[npc] = x
    npcy[npc] = y
    npcd[npc] = 1 
    npc += 1
    --mset(lx+x,ly+y,1)
   end 
  end
  
 end
 maxnpc = npc
end

function _endgame() 

 if btnp(4) and time() > last+1 then
  gamemode = 0
 end
end	

function	_maingameloop()
 _ply()
 _npc()
end 

function _npc()
 npcc = 0
	for npcc = 0,maxnpc do
  typ = mget(npcx[npcc],npcy[npcc])
  if npcd[npcc] == 4 then
	  npx = npcx[npcc]+1
	  npy = npcy[npcc]
	 end
	 if npcd[npcc] ==  2 then
	  npx = npcx[npcc]-1
	  npy = npcy[npcc]
	 end
	 if npcd[npcc] ==  3 then
	  npx = npcx[npcc] 
	  npy = npcy[npcc]-1
	 end
	 if npcd[npcc] ==  1 then
	  npx = npcx[npcc] 
	  npy = npcy[npcc]+1
	 end	 
  if mget(npx,npy) == 0 then
   mset(npx,npy,typ)
   mset(npcx[npcc],npcy[npcc],0)
			npcx[npcc] = npx
	  npcy[npcc] = npy
	  --mset(npcx[npcc],npcy[npcc],npco[npcc])
	 else 
   if typ > 48 then
	   --npcd[npcc] += 1
	   if npcd[npcc] == 4 then
	    npcd[npcc] = 1
	   else
		   if npcd[npcc] == 3 then
		    npcd[npcc] = 4
		   end
		   if npcd[npcc] == 2 then
		    npcd[npcc] = 3
		   end
		   if npcd[npcc] == 1 then
		    npcd[npcc] = 2
		   end
		  end
   end
  end
 end
end 
 
function	_ply() 
 nx = px
 ny = py
 if btnp(0) then nx = px - 1 end
	if btnp(1) then nx = px + 1 end
	if btnp(2) then ny = py - 1 end
	if btnp(3) then ny = py + 1 end
	if mget(nx,ny) == 0 or mget(nx,ny) == 6 then 
  mset(px,py,0)
	 px = nx
	 py = ny
	 mset(nx,ny,1)
	end
	if mget(nx,ny) == 18 then
  mset(px,py,0)
	 px = nx
	 py = ny
	 mset(nx,ny,1)
	 diamonds += 1
	end
	if mget(nx,ny) == 17 and px!= nx then
  if mget(nx+((nx-px)*2),ny) == 0 then
  mset(px,py,0)
  mset(nx+((nx-px)*2),17)
	 px = nx
	 py = ny
	 mset(nx,ny,1)
	 end
 end 
end
	
-- update the position based on the player pressing buttons
function _update()
	if gamemode == 0 then _intro() end	
	if gamemode == 1 then _titlescreen() end	
	if gamemode == 2 then _initgame() end	
	if gamemode == 3 then _maingameloop() end	
	if gamemode == 4 then _endgame() end	
	if gamemode == 5 then end	
end

function _draw()
	cls(0)
	if gamemode == 1 then 
  yt=26
	 for a=1,#title do
	  print (title[a],(64-(#title[a]*2)),yt)
	  yt += 8
	 end
	end
	if gamemode == 3 then
	  map(0,0,0,0,16,16)
	end
end 
