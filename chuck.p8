pico-8 cartridge // http://www.pico-8.com
version 8
__lua__


function _init()
	gamemode = 0
end



function _intro()
	gamemode = 1
end

function _titlescreen()
	gamemode = 2
end

function _initgame()
	lives = 3
	level = 1 
	gamemode = 3
end

function _initlevel()
 npc = 0 -- npc count
 npcx = {} -- npc y
 npcy = {} -- npc x
 npcd = {} -- npc direction
 calcy = flr(level/8)
 calcx = level - (calcy*8)

 lx = calcx * 16
 ly = calcy * 16
 
 for y=0, 15 do
  for x=0, 15 do
   if mget(lx+x,ly+y)<4 then mset(x,y,mget(lx+x,ly+y)) end
   -- find player 
   if mget(lx+x,ly+y)==20 then 
    px = x*8
    py = y*8
    pd = false
   end
   -- find npc
   if mget(lx+x,ly+y)==17 then
    npcx[npc] = x*8
    npcy[npc] = y*8
    npc += 1
   end 
  end 
 end
	gamemode = 4
end

function _maingameloop()
	_ply()
	_npc()
end

function _ply() 
	nx = px
 ny = py
 buttonpress = 0
 ---
 wx = flr((px+4)/8)
 wy = flr((py+7)/8)
 wy2 = flr((py+16)/8)
 wy3 = flr((py+14)/8)
 ---
 if mget(wx,wy3) == 3 then
  mset(wx,wy3,0)
 end
 
 
 --
 if btn(0) then 
  if mget(flr((px+1)/8),wy3) != 1 then 
  	nx = px - 1 
			pd = true
  end
 end
	if btn(1) then 
  if mget(flr((px+7)/8),wy3) != 1 then 
		 nx = px + 1 
 		pd = false
		end
	end
 -- 
	if mget(wx,wy) == 2 then
		if btn(2) then ny = py - 1 end

	end
	if mget(wx,wy+1) == 2 then
		if btn(3) then ny = py + 1 end
	end
	---
	if btn(4) then buttonpress = 1 end
 px = nx
 py = ny
 -- gravity
 if mget(wx,wy2) <1 or mget(wx,wy2) > 2  then
  py += 1
 end

end

function _npc() 
 for npcc= 0, npc do
 
 end
end

function _death()
	lives -= 1
	gamemode = 3
end

function _levelcomplete()
	level += 1
	gamemode = 3
end





function _update()
	if gamemode == 0 then _intro() end	
	if gamemode == 1 then _titlescreen() end	
	if gamemode == 2 then _initgame() end	
	if gamemode == 3 then _initlevel() end	
	if gamemode == 4 then _maingameloop() end	
end

function _draw()
	cls(0)
	map(0,0,0,0,16,16)
	_drawply()
	_drawnpc()
	_gui()
end

function _drawply()
	spr(20,px,py,1,2,pd)
	wx = flr((px+4)/8)
 wy = flr((py+16)/8)
 spr(16,wx*8,wy*8,1,1)
	wx = flr((px+4)/8)
 wy = flr((py+14)/8)
 spr(32,wx*8,wy*8,1,1)
end

function _drawnpc()
 for npcc= 0, npc-1 do
	 spr(17,npcx[npcc],npcy[npcc],1,2) 
 end

end

function _gui()
end


